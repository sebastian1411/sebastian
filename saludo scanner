package saludo;

import java.util.Scanner;

public class Saludo {

    public static void main(String[] args) {
        
        System.out.println("digita tu nombre");
        Scanner entrada = new Scanner (System.in );
        String nombre;
        nombre = entrada.nextLine();
        System.out.println("Hola "+ nombre);
        
    }
    
}
